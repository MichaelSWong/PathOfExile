<link rel="stylesheet" href="~/css/homeIndex.css" />
@model IList<CharacterModel>
@{
    ViewData["Title"] = "Home Page";
}

<img class="logo" src="~/images/render_full.png" />
<div class="wrapper">
    @foreach (CharacterModel character in Model)
    {
        string imageName = character.Classes_Name + "_avatar.png";

        <h3><a href="~/home/detail?id=@character.Classes_Name">@character.Classes_Name</a></h3>
        <h3><a href="~/home/detail?id=@character.Classes_Name"><img id="images" src="~/images/@imageName" /></a></h3>
        <div class="second">
            <h5>Starting Dex: @character.Starting_Dex</h5>
            <h5>Starting Int: @character.Starting_Int</h5>
            <h5>Starting Str: @character.Starting_Str</h5>
        </div>
        <hr />
    }
</div>